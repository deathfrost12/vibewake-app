{
  "name": "owlee",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "APP_VARIANT=development expo start --dev-client",
    "dev:lan": "APP_VARIANT=development expo start --dev-client --lan",
    "dev:localhost": "APP_VARIANT=development expo start --dev-client --localhost",
    "dev:tunnel": "APP_VARIANT=development expo start --dev-client --tunnel",
    "dev:clear": "APP_VARIANT=development expo start --dev-client --clear",
    "start:dev": "expo start --dev-client",
    "start:web": "expo start --web",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "android:dev": "expo run:android --device",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --device",
    "ios:sim": "expo run:ios --simulator",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios --profile development",
    "build:android": "eas build --platform android --profile development",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "submit": "eas submit --platform all",
    "test-screens": "./scripts/test-screens.sh",
    "lint": "echo 'TODO: Add ESLint'",
    "lint:fix": "echo 'TODO: Add ESLint with --fix'",
    "type-check": "tsc --noEmit",
    "ci:build": "./scripts/ci/build.sh",
    "ci:test": "./scripts/ci/test.sh",
    "ci:deploy": "./scripts/ci/deploy.sh",
    "quality": "./scripts/ci/test.sh quality",
    "fix": "./scripts/ci/test.sh fix",
    "pre-commit": "./scripts/ci/test.sh pre-commit",
    "dev-menu": "echo 'üß™ Open Dev Menu at /dev-menu in app'",
    "clear-cache": "npx expo start --clear && rm -rf node_modules/.cache",
    "clear-ios": "rm -rf ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData && cd ios && rm -rf Pods && pod install",
    "clear-android": "cd android && ./gradlew clean",
    "prebuild:ios": "npx expo prebuild --platform ios --clean",
    "prebuild:android": "npx expo prebuild --platform android --clean",
    "debug-google": "echo 'üîç Google OAuth Debug Info:' && echo 'Web Client ID:' && echo $EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID && echo 'Client Secret configured:' && [ -n \"$EXPO_PUBLIC_GOOGLE_CLIENT_SECRET\" ] && echo 'Yes' || echo 'No'",
    "debug-env": "echo 'üîç Environment Debug:' && echo 'App Environment:' && echo $EXPO_PUBLIC_APP_ENV && echo 'Bundle ID: com.yourcompany.yourapp.dev' && echo 'Scheme: yourapp-dev'",
    "diagnose-network": "echo 'üîç Network Diagnostics:' && echo 'Mac IP address:' && ifconfig | grep 'inet ' && echo 'Metro server:' && lsof -i :8081 && echo 'iPhone connected:' && xcrun devicectl list devices",
    "reset-metro": "pkill -f 'expo start' && echo 'Metro server stopped' && sleep 2 && echo 'Starting fresh Metro server...'",
    "help-connection": "echo 'üìö Connection Help:' && echo '1. iPhone Settings ‚Üí Privacy & Security ‚Üí Developer Mode' && echo '2. iPhone Settings ‚Üí App ‚Üí Local Network ‚Üí ON' && echo '3. npm run dev:lan' && echo '4. Shake iPhone ‚Üí Configure Metro ‚Üí 192.168.1.139:8081'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.50.1",
    "@tanstack/react-query": "^5.81.2",
    "expo": "53.0.20",
    "expo-apple-authentication": "~7.2.4",
    "expo-application": "~6.1.5",
    "expo-auth-session": "~6.2.1",
    "expo-av": "^15.1.7",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "~18.1.10",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.3",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "^14.2.0",
    "nativewind": "^4.1.23",
    "posthog-react-native": "^4.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-purchases": "^8.11.4",
    "react-native-purchases-ui": "^8.11.4",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.5",
    "react-native-webview": "13.13.5",
    "expo-task-manager": "~13.1.6",
    "expo-battery": "~9.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true,
  "description": "A production-ready React Native template built with Expo, TypeScript, and modern mobile development best practices",
