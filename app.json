{
  "expo": {
    "name": "Owlee",
    "slug": "owloo",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B7280"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.owlee.app",
      "jsEngine": "hermes",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to photos for image features",
        "NSCameraUsageDescription": "This app needs access to camera for photo features",
        "NSUserNotificationsUsageDescription": "This app needs notification permissions to send alarm notifications and wake you up at scheduled times",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio",
          "background-processing",
          "remote-notification"
        ]
      },
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"],
        "aps-environment": "development"
      },
      "associatedDomains": ["applinks:yourapp.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6B7280"
      },
      "package": "com.danielholes.vibewake",
      "jsEngine": "hermes",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.571576096718-poh1o01vj6k96k55895i85hq9vtn8rgo"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "note": "Use SENTRY_AUTH_TOKEN environment variable to authenticate with Sentry.",
          "organization": "yourorg",
          "project": "yourapp"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow VibeWake to access audio files for alarm sounds."
        }
      ],
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#5CFFF0",
          "defaultChannel": "alarms",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-task-manager",
      "expo-dev-client",
      "expo-localization"
    ],
    "scheme": "com.owlee.app",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "82251050-95f2-4754-b540-fc3c0517d847"
      }
    },
    "owner": "danielholes"
  }
}
